<h1>Listing meals</h1>

<table>
  <tr>
    <th>Venue</th>
    <th>Start</th>
    <th>End</th>
    <th>Type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @meals.each do |meal| %>
  <tr>
    <td><%= meal.venue %></td>
    <td><%= meal.venue_name %></td>
    <td><%= meal.start %></td>
    <td><%= meal.end %></td>
    <td><%= meal.type %></td>
    <td><%= link_to 'Show', meal %></td>
    <td><%= link_to 'Edit', edit_meal_path(meal) %></td>
    <td><%= link_to 'Destroy', meal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Meal', new_meal_path %>
